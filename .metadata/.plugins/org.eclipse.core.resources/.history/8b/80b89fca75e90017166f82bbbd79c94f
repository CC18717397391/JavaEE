  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet_url_pattern</servlet-name>
    <url-pattern>/HUP</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>HelloServlet_jsp_html</servlet-name>
    <servlet-class>servlet.HelloServlet_jsp_html</servlet-class>
    <init-param>
      <param-name>computer</param-name>
      <param-value>LENOVO</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloServlet_jsp_html</servlet-name>
    <url-pattern>/HJH</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>Forward_Jsp</servlet-name>
    <jsp-file>/Xml_Jspfile_Init.jsp</jsp-file>
    <init-param>
      <param-name>computer</param-name>
      <param-value>LENOVO</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Forward_Jsp</servlet-name>
    <url-pattern>/Forward_Jsp</url-pattern>
  </servlet-mapping>
  
  <error-page>
    <error-code>404</error-code>
    <location>/Error.jsp</location>
  </error-page>
  <error-page>
