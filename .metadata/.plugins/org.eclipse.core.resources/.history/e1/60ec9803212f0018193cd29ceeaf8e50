function Validate(){
	$("#Tip").empty();
	var account = $("#account").val();	//获取form表单中account的值
	var password = $("#password").val();//获取form表单中password的值
	if(account == "" || password == ""){
		$("#Tip").append("用户名或密码不能为空");
		return;
	}
	$.ajax({
	    url:'login/validate',
	    type:'post', 	//GET 数据发送方式
	    async:true,     //异步发送，false为同步
	    data:{ key_account:account, key_password:password}, //发送到后台的数据， 以map映射方式
	    timeout:5000,    //超时时间
	    dataType:'json', //返回数据类型
	    success:function(data){		//返回成功后执行的方法
	    	window.location.href='https://123.sogou.com/';
	    }
	})
	
}
